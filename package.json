  "description": "Prototypo, kickstarting font creation",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:dist": "gulp build",
    "build:storybook": "build-storybook",
    "debug": "gulp debug",
    "start": "gulp serve",
    "start:merge": "MERGE=true gulp serve",
    "start:perf": "gulp serve:perf",
    "storybook": "start-storybook -p 6006",
    "test": "NODE_PATH=$pwd/node_modules:$(pwd)/app jest",
    "analyze": "webpack --config analyze.config.js",
    "analyze:prod": "webpack --config analyze-prod.config.js",
    "build:lib": "webpack --config library.config.js",
    "build:precursor": "webpack --config precursor.config.js",
    "lint:files": "prettier-eslint $(find ./app/scripts -type f -regex '.*\\.jsx?') stories/** *.config.js .babelrc .eslintrc",
    "lint": "npm run lint:files -- --list-different",
    "format": "npm run lint:files -- --write",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/byte-foundry/prototypo.git"
  },
  "author": "YM & LRB",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/byte-foundry/prototypo/issues"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "useTabs": true,
    "bracketSpacing": false
  },
  "lint-staged": {
    "stories/**": [
      "prettier-eslint --write",
      "git add"
    ],
    "app/scripts/**/**.{js,jsx}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
